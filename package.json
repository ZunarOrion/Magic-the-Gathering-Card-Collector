  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@types/express": "^5.0.3",
    "@types/node": "^24.7.0",
    "mongodb-memory-server": "^10.2.3",
    "typescript": "^6.0.0-dev.20250825",
    "vite": "^7.1.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "express": "^5.1.0",
    "mongodb": "^6.19.0",
    "tsx": "^4.20.6"
  }
}
